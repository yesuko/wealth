    "version": "0.2.0",
    "configurations": [
        {
            "name": "wealth (local emulator)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "debug",
            "toolArgs": [
                "--dart-define",
                "USE_FIREBASE_EMU=true",
            ]
        },
        {
            "name": "wealth (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "wealth (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release",
            "toolArgs": [
                "--dart-define",
                "USE_FIREBASE_EMU=false",
            ]
        }
    ]
}


